class Solution {
    public int[] shuffle(int[] nums, int n) {
        int[] answer = new int[2*n];
        int pos = 0;
        for(int i = 0; i < nums.length; i+=2){
            answer[i] = nums[pos]; 
            answer[i+1] = nums[pos+n];
            pos++;
        }
        return answer;
    }
}