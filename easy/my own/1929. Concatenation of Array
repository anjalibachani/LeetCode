class Solution {
    public int[] getConcatenation(int[] nums) {
        int len = nums.length;
        int ans_len = 2*len;
        int[] answer = new int[ans_len];
        for(int j = 0; j < len; j++){
            answer[j] = nums[j];
            answer[j + len] = nums[j];
        }
        return answer;
    }
}