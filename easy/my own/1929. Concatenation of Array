class Solution {
    public int[] getConcatenation(int[] nums) {
        int len = nums.length;
        int lengthy = 2 * len;
        int[] answer = new int[lengthy];
        int i = 0;

        while(i < lengthy){
            for(int j = 0; j < len; j++){
                answer[i] = nums[j];
                i++;
            }
        }
        System.out.println("answer = " + Arrays.toString(answer));
        return answer;
    }
}