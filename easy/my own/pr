class Solution {
    public int[] twoSum(int[] nums, int target) {
        HashMap<Integer, Integer> hm = new HashMap<Integer, Integer>();
        int[] ans = new int[2];

        for(int i = 0; i < nums.length; i++) {
            if(hm.containsKey(target - nums[i])) {
                ans = new int[]{hm.get(target - nums[i]), i};
            }
            hm.put(nums[i], i);
        }
        return ans;
    }
}