class Solution {
    public int[] smallerNumbersThanCurrent(int[] nums) {
        int[] answer = new int[nums.length];
        for(int i = 0; i < nums.length; i++){
            int increment = 0;
            for(int j = 0; j < nums.length; j++){
                if(i != j){
                    if(nums[i] > nums[j]){
                        increment++;
                    }
                }
                else{
                    continue;
                }
            }
            answer[i] = increment;
        }
        return answer;
    }
}